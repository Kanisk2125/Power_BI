	column Risk_Category = ```
			
			IF('Customer_Details'[Credit_Score] < 580, "High Risk",
			    IF('Customer_Details'[Credit_Score] < 670, "Moderate Risk",
			        IF('Customer_Details'[Credit_Score] < 740, "Low Risk", "Very Low Risk")
			    )
			)
			
			```
		lineageTag: 6b50ba89-68f9-44c3-8865-0bde819f2b54
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column Income_Category = ```
			
			SWITCH(
			    TRUE(),
			    'Customer_Details'[Income] < 30000, "Low",
			    'Customer_Details'[Income] < 70000, "Medium",
			    "High"
			)
			
			```
		lineageTag: c67ca7e0-0cec-4ecc-9a1d-7fcc8f209108
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column 'Credit Score Bucket' = ```
			
			SWITCH(
			    TRUE(),
			    'Customer_Details'[Credit_Score] < 580, "Poor",
			    'Customer_Details'[Credit_Score] < 670, "Fair",
			    'Customer_Details'[Credit_Score] < 740, "Good",
			    'Customer_Details'[Credit_Score] < 800, "Very Good",
			    "Excellent"
			)
			
			```
		lineageTag: 951f4847-ac2a-4a1a-8f06-ff154b832098
